{
  "name": "@metaxia/scriptures-source-crosswire-kjv",
  "version": "0.1.1",
  "description": "King James Version (English) data for @metaxia/scriptures. Source: CrossWire Bible Society.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.js"
    },
    "./metadata": {
      "types": "./data/crosswire-KJV/metadata.json",
      "import": "./data/crosswire-KJV/metadata.json"
    },
    "./data/*": "./data/*",
    "./cache/*": "./cache/*",
    "./books/*": {
      "types": "./dist/books/*.d.ts",
      "import": "./dist/books/*.js"
    }
  },
  "files": [
    "dist",
    "data",
    "cache"
  ],
  "scripts": {
    "build": "tsc",
    "import": "tsx scripts/import.ts",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "bible",
    "kjv",
    "king-james-version",
    "scriptures",
    "crosswire"
  ],
  "author": "Benjamin Plunkert",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-crosswire-kjv"
  },
  "bugs": {
    "url": "https://github.com/metaxiamultimedia/scriptures-js-source-crosswire-kjv/issues"
  },
  "homepage": "https://github.com/metaxiamultimedia/scriptures-js-source-crosswire-kjv#readme",
  "peerDependencies": {
    "@metaxia/scriptures-core": ">=0.1.1"
  },
  "devDependencies": {
    "@metaxia/scriptures-core": "^0.1.1",
    "fast-xml-parser": "^4.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.0"
  },
  "dependencies": {
    "saxes": "^6.0.0"
  }
}
